<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:n1="http://schemas.android.com/apk/res/android" xmlns:n2="http://schemas.android.com/apk/res-auto">
    <item n1:id="@android:id/background" n1:drawable="@drawable/abc_scrubber_track_mtrl_alpha"/>
    <item n1:id="@android:id/secondaryProgress">
        <scale n1:scaleWidth="100%">
            <selector>
                <item n1:state_enabled="false">
                    <color n1:color="@android:color/transparent"/>
                </item>
                <item n1:drawable="@drawable/abc_scrubber_primary_mtrl_alpha"/>
            </selector>
        </scale>
    </item>
    <item n1:id="@android:id/progress">
        <scale n1:scaleWidth="100%">
            <selector>
                <item n1:state_enabled="false">
                    <color n1:color="@android:color/transparent"/>
                </item>
                <item n1:drawable="@drawable/abc_scrubber_primary_mtrl_alpha"/>
            </selector>
        </scale>
    </item>
</layer-list>
